#include <stdio.h>

int main()
{
 int a,b ,sum;
 printf("Enter the value of a and b \n");
 scanf("%d%d",&a,&b);
 sum = a + b;
 printf(" sum of a amd b is %d\n", sum);
 if(sum > 20)
{printf("sum is greater than 20 \n");
}
else if (sum > 10)
{printf("sum is greater 10less20\n");
}
else
{printf("sum of less than 10 \n");
}
return 0;
}
